<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.vluver.cbj.colegio">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="m">
        <activity
            android:name=".Docente.DocenteActivity"
            android:theme="@style/DocenteStyle" />
        <activity
            android:name=".Estudiante.EstudianteActivity"
            android:theme="@style/EstudianteStyle" />
        <activity
            android:name=".Estudiante.SeleccionCursoActivity"
            android:theme="@style/EstudianteStyle" />
        <activity
            android:name=".Docente.SesionDocente"
            android:theme="@style/DocenteStyle" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>